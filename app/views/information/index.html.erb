<h1>Listing information</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Garage name</th>
      <th>Total parking space</th>
      <th>Position</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @information.each do |information| %>
      <tr>
        <td><%= image_tag information.image.url(:square) %></td>
        <td><%= information.garage_name %></td>
        <td><%= information.total_parking_space %></td>
        <td><%= information.position %></td>
        <td><%= information.longitude %></td>
        <td><%= information.latitude %></td>
        <td><%= link_to 'Show', information %></td>
        <td><%= link_to 'Edit', edit_information_path(information) %></td>
        <td><%= link_to 'Destroy', information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Information', new_information_path %>
